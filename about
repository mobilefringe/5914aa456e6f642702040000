    $(document).ready(function(e){
        init(e);
        loadMallData(renderAll); 
    })
    
    function renderAll(){
        var prop_details = getPropertyDetails();
        renderGeneral('#address_container','#address_template',prop_details);
        show_content();
    }
</script>